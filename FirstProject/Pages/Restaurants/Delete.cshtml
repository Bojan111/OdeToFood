@page "{Id:int}"
@model FirstProject.DeleteModel
@{
    ViewData["Title"] = "Delete";
    Layout = "_Layout";
}


<h2>Delete</h2>
<div class="alert alert-danger">
    Are you sure you want do delete @Model.Restourant.Name?
</div>
<form method="post">
    <button type="submit" class="btn btn-danger">Yes</button>
    <a asp-page="List" class="btn btn-outline-primary">Cancel</a>
</form>

